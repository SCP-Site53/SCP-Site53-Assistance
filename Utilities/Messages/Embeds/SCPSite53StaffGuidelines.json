                        {
                            "title": "SCP Site53 Staff Guidelines",
                            "description": "The Staff Guidelines help ensure that Site53 is a safe and respectable place. You must follow the guidelines or we will punish you.",
                            "color": 8323072,
                            "author": {
                                "name": "SCP Site53 Management",
                                "icon_url": "https://cdn.discordapp.com/attachments/992969548598870067/1019437585149464616/unknown.png"
                            },
                            "footer": {
                              "text": "​"
                            }
                        },
                        {
                            "color": 8323072,
                            "fields": [
                                {
                                    "name": "1. Gammar",
                                    "value": "> You must use grammar (*no slag*) when you are not talking or messaging in a staf-only zone.",
                                    "inline": true
                                },
                                {
                                    "name": "2. Abuse",
                                    "value": "> Do not abuse your powers in any way. If you abuse your powers, we will fire and ban you.",
                                    "inline": true
                                }
                            ],
                            "footer": {
                              "text": "​"
                            }
                        },
                        {
                            "color": 8323072,
                            "fields": [
                                {
                                    "name": "3. Player Guidelines",
                                    "value": "> You must abide by all SCP Site53 player guidelines. Staff are not immune from moderation.",
                                    "inline": true
                                },
                                {
                                    "name": "4. Complaining",
                                    "value": "> Do not complain about a promotion or demotion. It ruins other people's mood.",
                                    "inline": true
                                }
                            ],
                            "footer": {
                              "text": "​"
                            }
                        },
                        {
                            "color": 8323072,
                            "fields": [
                                {
                                    "name": "5. Command Usage",
                                    "value": "> You must use the full username when kicking or banning a player. You can only use \":message\" commands when advertising the Discord server or during a lecture (STS).",
                                    "inline": true
                                },
                                {
                                    "name": "6. Advertising the Discord Server",
                                    "value": "> You must advertise the SCP Site53 Discord Server every 15 minutes while on duty. Otherwise, we will reducate you on the proper ways to staff on SCP Site53, so you will become a trainee (Dirt Moderator).",
                                    "inline": true
                                }
                            ],
                            "footer": {
                              "text": "​"
                            }
                        },
                        {
                            "color": 8323072,
                            "fields": [
                                {
                                    "name": "7. Logging",
                                    "value": "> You must log all punishments. If you do not log punishments, we will retrain you and you will be a trainee (Dirt Moderator).",
                                    "inline": true
                                },
                                {
                                    "name": "8. Quota",
                                    "value": "> You must complete a 2-4 hour quota each week. If you are lazy enough to not complete your quota, we will strike you.",
                                    "inline": true
                                }
                            ],
                            "footer": {
                              "text": "​"
                            }
                        },
                        {
                            "color": 8323072,
                            "fields": [
                                {
                                    "name": "9. Be Professional",
                                    "value": "> Refrain from screwing around, saying lame jokes, or act stupid while on duty.",
                                    "inline": true
                                },
                                {
                                    "name": "10. Be Appropriate",
                                    "value": "> No racist, sexual, or derogatory status displays, names, biographies, or profile pictures.",
                                    "inline": true
                                }
                            ],
                            "footer": {
                              "text": "​"
                            }
                        },
                        {
                            "color": 8323072,
                            "fields": [
                                {
                                    "name": "11. Intellectual Property",
                                    "value": "> Do not distribute SCP Site53 Intellectual Property. Everything stays hidden for a good reason.",
                                    "inline": true
                                },
                                {
                                    "name": "12. Outside of SCP Site53",
                                    "value": "> Do not harassing or bullying a person outside of SCP Site53. If you do, we will peacfully fire and ban you.",
                                    "inline": true
                                }
                            ],
                            "footer": {
                              "text": "​"
                            }
                        }
